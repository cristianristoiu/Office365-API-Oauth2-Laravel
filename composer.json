{
    "name": "Miguel_Costa/office365api",
    "type": "library",
    "description": "First test of connection to Microsoft Graph API 1.0",
    "keywords": [
        "Miguel_Costa",
        "Office365API"
    ],
    "homepage": "https://github.com/Miguel_Costa/Office365API",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Costa",
            "email": "tga_miguel_tga@hotmail.com",
            "homepage": "https://github.com/killmi",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "league/oauth2-client": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Miguel_Costa\\Office365API\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Miguel_Costa\\Office365API\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
